write("Hello, World!");
write("Enter your name:");
let weird = func (x, y) {
	out = (x + y) * (x - y);
};
let name = read();
write("Hello, ", name);
write("two plus two is ", 2 + 2);
if name is "Admin" {
	write("ACCESS GRANTED!");
	let correct = 1;
	loop correct is 1 {
		write("What is (2 + 2) * (2 - 2)?");
		let answer = read();
		if answer not "0" {
			correct = 0;
		};
	};
	write("INCORRECT!", " TWO PLUS TWO IS ", weird(2, 2));
};
if name not "Admin" {
	write("ACCESS DENIED!");
};